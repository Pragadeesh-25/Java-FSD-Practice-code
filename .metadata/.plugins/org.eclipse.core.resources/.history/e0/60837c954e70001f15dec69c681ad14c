package pack;

public abstract class WrittenItem extends Item{
	private int authorName;
	private int noBookspub;
	

	public int getAuthorName() {
		return authorName;
	}


	public void setAuthorName(int authorName) {
		this.authorName = authorName;
	}


	public int getNoBookspub() {
		return noBookspub;
	}


	public void setNoBookspub(int noBookspub) {
		this.noBookspub = noBookspub;
	}


	public WrittenItem(int uid, String name, int noc) {
		super(uid, name, noc);
		// TODO Auto-generated constructor stub
	}

}
