package bank;

public abstract class SBAccount extends Account{
	String type;
	public SBAccount(double amount,String type) {
		super(amount);
		// TODO Auto-generated constructor stub
		this.type=type;
	}
	//double interestRate;
	//double amount;
	
	 double calculateInterest()
	 {
		 if(this.type.equals("Normal"))
			 interestRate=4;
		 if(this.type.equals("NRI"))
			 interestRate=6;
		 return ((amount*interestRate)/100);
		 
			 
	 }
}
