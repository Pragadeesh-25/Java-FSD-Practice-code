package practice;

import java.util.ArrayList;

public class Student implements Comparable<Student>{
	String name;
	int marks;
	public Student(String name, int marks) {
		super();
		this.name = name;
		this.marks = marks;
	}
	@Override
	public int compareTo(Student o) {
		// TODO Auto-generated method stub
		return o.marks-this.marks;
	}
	

}
